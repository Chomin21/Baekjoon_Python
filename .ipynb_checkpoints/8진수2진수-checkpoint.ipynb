   "source": [
    "#my solution\n",
    "\n",
    "n = input()\n",
    "\n",
    "#입력받은 문자열을 8진수 형태로 만들기\n",
    "n = '0o' + n\n",
    "\n",
    "#8진수를 10진수로 변환\n",
    "oc = int(n, 8)\n",
    "\n",
    "#10진수를 2진수로 변환\n",
    "binary = bin(oc)\n",
    "\n",
    "#앞의 두자리 제외하고 읽기\n",
    "answer = binary[2:]\n",
    "print(answer)"
   ]
  },
