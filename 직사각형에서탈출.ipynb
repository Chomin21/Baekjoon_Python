   "source": [
    "#직사각형에서 탈출\n",
    "#escape from Rectangle\n",
    "\n",
    "x, y, w, h = map(int, input().split())\n",
    "min_x = 0\n",
    "min_y = 0\n",
    "\n",
    "if x <= (w - x):\n",
    "    min_x = x\n",
    "else:\n",
    "    min_x = w - x\n",
    "\n",
    "if y <= (h - y):\n",
    "    min_y = y\n",
    "else:\n",
    "    min_y = h - y\n",
    "\n",
    "print(min(min_x, min_y))\n",
    "\n",
    "#이 식이면 한번에 처리 가능\n",
    "#print(min(x, y, w - x, h - y))\n"
   ]
